{
  "name": "openrouter-crew-platform",
  "version": "1.0.0",
  "private": true,
  "description": "Unified cost-optimized AI orchestration platform with n8n, Supabase, and crew-based intelligence",
  "repository": {
    "type": "git",
    "url": "https://github.com/bradygeorgen/openrouter-crew-platform.git"
  },
  "scripts": {
    "dev": "concurrently \"pnpm dev:dashboards\" \"pnpm dev:supabase\"",
    "dev:dashboards": "concurrently \"pnpm --filter unified-dashboard dev\" \"pnpm --filter alex-ai-universal-dashboard dev --port 3001\" \"pnpm --filter dj-booking-dashboard dev --port 3002\" \"pnpm --filter test-event-venue-dashboard dev --port 3003\"",
    "dev:dashboard": "pnpm --filter unified-dashboard dev",
    "dev:supabase": "supabase start",
    "dev:all": "docker-compose up",
    "build": "pnpm fix:tsconfig && turbo build",
    "build:dashboards": "turbo run build --filter=unified-dashboard --filter=alex-ai-universal-dashboard --filter=dj-booking-dashboard --filter=test-event-venue-dashboard",
    "build:dashboards:prod": "pnpm run build:dashboards && echo 'All dashboards built successfully'",
    "build:domain": "bash scripts/build.sh",
    "build:dashboard": "pnpm --filter unified-dashboard build",
    "type-check": "pnpm -r type-check",
    "fix:tsconfig": "node scripts/system/fix-tsconfig-corruption.js",
    "fix:deps": "bash scripts/system/fix-lucide-deps.sh",
    "lint": "pnpm -r lint",
    "test": "pnpm -r test",
    "test:integration": "pnpm --filter @openrouter-crew/integration-tests test",
    "test:e2e": "pnpm --filter @openrouter-crew/e2e-tests test",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:reset": "supabase db reset",
    "supabase:status": "supabase status",
    "db:migrate": "supabase db push",
    "db:seed": "supabase db seed",
    "n8n:sync": "node scripts/n8n/sync-workflows.js",
    "n8n:sync:prod": "node scripts/n8n/sync-workflows.js --prod",
    "n8n:export": "node scripts/n8n/export-workflows.js",
    "n8n:activate": "node scripts/n8n/activate-workflows.js",
    "n8n:verify": "node scripts/n8n/verify-webhooks.js",
    "n8n:backup": "bash scripts/n8n/backup-workflows-cli.sh",
    "secrets:sync": "bash scripts/secrets/sync-from-zshrc.sh",
    "secrets:validate": "node scripts/secrets/validate-env.js",
    "deploy:local": "docker-compose up -d",
    "git:verify": "bash scripts/git/verify-git-status.sh",
    "git:setup": "node scripts/git/setup-remote.js",
    "deploy:aws": "bash scripts/deploy/deploy-aws.sh",
    "deploy:vercel": "pnpm --filter unified-dashboard vercel",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "docker-compose down -v && docker system prune -f",
    "feature:create": "bash scripts/agile/create-feature.sh",
    "feature:push": "bash scripts/agile/push-feature.sh",
    "story:create": "bash scripts/agile/create-story.sh",
    "story:push": "bash scripts/agile/push-story.sh",
    "milestone:create": "bash scripts/agile/create-milestone.sh",
    "milestone:push": "bash scripts/agile/push-milestone.sh",
    "clean": "pnpm -r clean && rm -rf node_modules",
    "organize": "bash scripts/organize-workspace.sh",
    "reset": "bash scripts/reset-build.sh",
    "setup": "pnpm install && pnpm secrets:sync && pnpm supabase:start",
    "domain:create": "bash scripts/domain/create-domain.sh",
    "sync:all": "bash scripts/system/sync-all.sh",
    "generate:types": "bash scripts/supabase/generate-types.sh"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "concurrently": "^8.2.2",
    "turbo": "^2.0.0",
    "typescript": "^5.9.3",
    "tailwindcss": "^3.4.1"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.12.3",
  "pnpm": {
    "overrides": {
      "react": "18.3.1",
      "react-dom": "18.3.1"
    }
  },
  "dependencies": {
    "supabase": "^2.72.9"
  }
}
