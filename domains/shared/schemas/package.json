{
  "name": "@openrouter-crew/shared-schemas",
  "version": "1.0.0",
  "description": "TypeScript types generated from Supabase database schema",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "generate": "supabase gen types typescript --local > src/database.ts",
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "typescript": "^5.9.3"
  }
}
