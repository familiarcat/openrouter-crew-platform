      "parameters": {
        "functionCode": "// Validate and transform user settings\nconst body = $input.item.json.body;\n\n// Use 'default' user for MVP (single-user system)\nconst userId = body.userId || 'default';\n\n// Validate globalTheme is provided\nif (!body.globalTheme) {\n  throw new Error('globalTheme is required');\n}\n\n// Build settings data\nconst settingsData = {\n  user_id: userId,\n  global_theme: body.globalTheme,\n  preferences: body.preferences ? JSON.stringify(body.preferences) : '{}',\n  updated_at: new Date().toISOString()\n};\n\nconsole.log('⚙️ Storing settings for user:', settingsData.user_id, '| Theme:', settingsData.global_theme);\n\nreturn { json: settingsData };"
      },
