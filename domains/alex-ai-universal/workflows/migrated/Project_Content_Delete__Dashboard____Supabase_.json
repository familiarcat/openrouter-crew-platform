      ]
    },
    {
      "id": "supabase-soft-delete",
      "type": "supabaseQuery",
      "config": {
        "name": "Supabase Soft Delete",
        "parameters": {
          "operation": "update",
          "table": "projects",
          "updateKey": "project_id",
