      "type": "transform",
      "config": {
        "name": "Response Success",
        "parameters": {
          "respondWith": "json",
          "responseBody": "={{ { \"success\": true, \"projectId\": $('Validate Input').item.json.project_id, \"message\": \"Project deleted successfully\", \"timestamp\": $now } }}",
